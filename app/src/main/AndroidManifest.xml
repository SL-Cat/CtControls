<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cat.hucustomize">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ct_ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ct_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".list.RecyerActivity"></activity>
        <activity android:name=".list.XListViewActivity"></activity>
    </application>

</manifest>