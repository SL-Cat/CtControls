<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="text" format="string" />
    <attr name="text_size" format="dimension" />
    <attr name="image" format="integer" />
    <attr name="background" format="reference"/>
    <attr name="ct_padding_left" format="integer"></attr>
    <attr name="ct_padding_top" format="integer"></attr>
    <attr name="ct_padding_Bottom" format="integer"></attr>
    <attr name="ct_padding_right" format="integer"></attr>
    <attr name="right_text" format="string" />
    <attr name="left_text" format="string" />

    <declare-styleable name="ISelectBtnStyle">
        <attr name="left_text"/>
        <attr name="left_text_background" format="reference"/>
        <attr name="right_text_background" format="reference"/>
        <attr name="right_text"/>
    </declare-styleable>

    <declare-styleable name="ISpringStyle">
        <attr name="spring_text" format="string" />
        <attr name="spring_image" format="integer" />
    </declare-styleable>

    <declare-styleable name="IReadCleanPowerStyle">
        <attr name="start_text" format="string" />
        <attr name="stop_text" format="string" />
        <attr name="reset_text" format="string" />
        <attr name="start_text_color" format="color" />
        <attr name="stop_text_color" format="color" />
        <attr name="reset_text_color" format="color" />
        <attr name="power_text_color" format="color" />
        <attr name="start_background" format="reference" />
        <attr name="stop_background" format="reference" />
        <attr name="reset_background" format="reference" />
        <attr name="power_background" format="reference" />
        <attr name="button_height" format="dimension" />
    </declare-styleable>

    <declare-styleable name="IScanResetReadStyle">
        <attr name="scan_text" format="string" />
        <attr name="read_text" format="string" />
        <attr name="mid_image" format="integer" />
        <attr name="mid_padding" format="integer" />
        <attr name="left_background" format="reference" />
        <attr name="right_background" format="reference" />
        <attr name="scan_read_height" format="dimension" />
    </declare-styleable>

    <declare-styleable name="IRadiosStyle">
        <attr name="item_background" format="reference"></attr>
        <attr name="item_text_un_color" format="color" />
        <attr name="item_text_click_color" format="color" />
        <attr name="item_text_size" format="dimension" />
        <attr name="radios_click_more" format="boolean" />
    </declare-styleable>

    <declare-styleable name="IToolBarStyle">
        <attr name="title_text" format="string"/>
        <attr name="toolbar_background" format="reference"/>
        <attr name="left_image" format="reference"/>
        <attr name="right_image" format="reference"/>
        <attr name="right_text" />
        <attr name="hide_right_image" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ISwitchStyle">
        <attr name="switch_button_color" format="color"/>
        <attr name="switch_button_height" format="float"/>
        <attr name="switch_button_click" format="boolean"/>
        <attr name="text"/>
        <attr name="text_size"/>
    </declare-styleable>

    <declare-styleable name="IDatePickerStyle">
        <attr name="select_text_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="IRadioEditStyle">
        <attr name="radio_edit_input" format="string"/>
        <attr name="radio_edit_text" format="string"/>
        <attr name="radio_edit_button" format="boolean"/>
        <attr name="radio_edit_left_image" format="reference"/>
        <attr name="radio_left_image_status" format="boolean"/>
        <attr name="radio_edit_right_image" format="reference"/>
        <attr name="text_size"/>
        <attr name="background"/>
    </declare-styleable>

    <declare-styleable name="CtLoopView">
        <attr format="integer" name="awv_textSize"/>
        <attr format="float" name="awv_lineSpace"/>
        <attr format="integer" name="awv_centerTextColor"/>
        <attr format="integer" name="awv_outerTextColor"/>
        <attr format="integer" name="awv_dividerTextColor"/>
        <attr format="integer" name="awv_itemsVisibleCount"/>
        <attr format="boolean" name="awv_isLoop"/>
        <attr format="integer" name="awv_initialPosition"/>
        <attr format="float" name="awv_scaleX"/>
    </declare-styleable>

    <declare-styleable name="ICtSeekBarStyle">
        <attr name="progress_max" format="float|reference"/> <!-- 进度条最大值 -->
        <attr name="progress_min" format="float|reference"/> <!-- 进度条最小值 -->
        <attr name="progress_default" format="float|reference"/> <!-- 进度条默认值 -->

        <attr name="track_left_height" format="dimension|reference"/> <!-- 进度条左边高度 -->
        <attr name="track_right_height" format="dimension|reference"/> <!-- 进度条右边高度 -->

        <attr name="track_left_color" format="color|reference"/> <!-- 进度条左边颜色 -->
        <attr name="track_right_color" format="color|reference"/> <!-- 进度条右边颜色 -->

        <attr name="thumb_color_default" format="color|reference"/> <!-- 拖动滑块默认颜色 -->
        <attr name="thumb_radius_default" format="dimension|reference"/> <!-- 拖动滑块半径 -->

        <attr name="thumb_color_on_dragging" format="color|reference"/> <!-- 拖动滑块拖动中颜色 -->
        <attr name="thumb_radius_on_dragging" format="dimension|reference"/> <!-- 拖动滑块拖动中半径 -->

    </declare-styleable>

</resources>